<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="me.sheepyang.qlady">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".QApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar.FullScreen"
        tools:replace="android:icon, android:theme">
        <activity
            android:name=".activity.HomePageActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.detail.ModelPhotoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.ModelListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.MineActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.SearchActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.detail.ModelDetailActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.BuyVIPActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.CollectionActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.EditInfoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.EditNameActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.EditPasswordActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.FeedbackActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.AboutActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.UserAgreementActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.login.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.ImageBrowserActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>