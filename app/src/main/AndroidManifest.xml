<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="me.sheepyang.qlady">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".QApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar.FullScreen"
        tools:replace="android:icon, android:theme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ModelPhotoActivity"/>
        <activity android:name=".activity.ModelListActivity"/>
        <activity android:name=".activity.mine.MineActivity"/>
        <activity android:name=".activity.SearchActivity"/>
        <activity android:name=".activity.ModelDetailActivity"/>
        <activity android:name=".activity.mine.BuyVIPActivity"/>
        <activity android:name=".activity.mine.CollectionActivity"/>
        <activity android:name=".activity.mine.EditInfoActivity"/>
        <activity android:name=".activity.mine.EditNameActivity">
        </activity>
        <activity android:name=".activity.mine.EditPasswordActivity">
        </activity>
        <activity android:name=".activity.mine.FeedbackActivity">
        </activity>
        <activity android:name=".activity.mine.AboutActivity">
        </activity>
        <activity android:name=".activity.mine.UserAgreementActivity">
        </activity>
        <activity android:name=".activity.login.LoginActivity">
        </activity>
    </application>

</manifest>